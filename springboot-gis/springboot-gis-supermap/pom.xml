<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.zja</groupId>
        <artifactId>springboot-gis</artifactId>
        <version>2.0-SNAPSHOT</version>
    </parent>
    <groupId>com.zja</groupId>
    <artifactId>springboot-gis-supermap</artifactId>

    <packaging>jar</packaging>

    <repositories>
        <repository>
            <id>supermap</id>
            <url>https://maven.supermap.io/</url>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
            <releases>
                <enabled>true</enabled>
            </releases>
        </repository>
    </repositories>

    <properties>
        <java.version>1.8</java.version>
        <!--supermap10已存在公司私服-->
        <!--<supermap.version>10.2.0-SNAPSHOT</supermap.version>-->
        <!--supermap11本地仓库-->
        <supermap.version>11.1.1</supermap.version>
    </properties>

    <dependencies>
        <!--超图数据模块-->
        <dependency>
            <groupId>com.supermap.data</groupId>
            <artifactId>data</artifactId>
            <version>${supermap.version}</version>
        </dependency>
        <!--超图云许可模块: 要与云许可一致-->
        <dependency>
            <groupId>com.supermap.data</groupId>
            <artifactId>cloudlicense</artifactId>
            <version>${supermap.version}</version>
        </dependency>
        <!--数据转换模块-->
        <dependency>
            <groupId>com.supermap.data</groupId>
            <artifactId>conversion</artifactId>
            <version>${supermap.version}</version>
        </dependency>
        <!--数据处理模块-->
        <dependency>
            <groupId>com.supermap.data</groupId>
            <artifactId>processing</artifactId>
            <version>${supermap.version}</version>
        </dependency>
        <!--拓扑模块-->
        <dependency>
            <groupId>com.supermap.data</groupId>
            <artifactId>topology</artifactId>
            <version>${supermap.version}</version>
        </dependency>
        <!--地图模块-->
        <dependency>
            <groupId>com.supermap.mapping</groupId>
            <artifactId>mapping</artifactId>
            <version>${supermap.version}</version>
        </dependency>
        <!--排版打印模块-->
        <dependency>
            <groupId>com.supermap.layout</groupId>
            <artifactId>layout</artifactId>
            <version>${supermap.version}</version>
        </dependency>
        <!--空间分析模块-->
        <dependency>
            <groupId>com.supermap.analyst</groupId>
            <artifactId>spatialanalyst</artifactId>
            <version>${supermap.version}</version>
        </dependency>
        <!--网络分析模块-->
        <dependency>
            <groupId>com.supermap.analyst</groupId>
            <artifactId>networkanalyst</artifactId>
            <version>${supermap.version}</version>
        </dependency>
        <!--地形分析模块-->
        <dependency>
            <groupId>com.supermap.analyst</groupId>
            <artifactId>terrainanalyst</artifactId>
            <version>${supermap.version}</version>
        </dependency>
        <!--地址匹配模块-->
        <dependency>
            <groupId>com.supermap.analyst</groupId>
            <artifactId>addressmatching</artifactId>
            <version>${supermap.version}</version>
        </dependency>
        <!--导航分析模块-->
        <dependency>
            <groupId>com.supermap.analyst</groupId>
            <artifactId>navigation</artifactId>
            <version>${supermap.version}</version>
        </dependency>
        <!--控件模块-->
        <dependency>
            <groupId>com.supermap.ui</groupId>
            <artifactId>controls</artifactId>
            <version>${supermap.version}</version>
        </dependency>

        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
            <exclusions>
                <!--解决cloudlicense.jar 包中 log4j 问题-->
                <exclusion>
                    <groupId>org.springframework.boot</groupId>
                    <artifactId>spring-boot-starter-logging</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-test</artifactId>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
            <optional>true</optional>
        </dependency>
    </dependencies>
</project>
