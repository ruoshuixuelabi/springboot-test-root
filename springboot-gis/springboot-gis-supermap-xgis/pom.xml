<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.zja</groupId>
        <artifactId>springboot-gis</artifactId>
        <version>2.0-SNAPSHOT</version>
    </parent>
    <groupId>com.zja</groupId>
    <artifactId>springboot-gis-supermap-xgis</artifactId>

    <packaging>jar</packaging>

    <properties>
        <java.version>1.8</java.version>
        <supermap.version>10.2.0-SNAPSHOT</supermap.version>
    </properties>

    <dependencies>
        <!-- xgis自定义接口模块-->
        <dependency>
            <groupId>com.dist.xdata</groupId>
            <artifactId>xgis-core</artifactId>
            <version>1.0-SNAPSHOT</version>
        </dependency>
        <!-- xgis-supermap实现模块-->
        <dependency>
            <groupId>com.dist.xdata</groupId>
            <artifactId>xgis-supermap</artifactId>
            <version>1.0-SNAPSHOT</version>
        </dependency>

        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-test</artifactId>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
            <optional>true</optional>
        </dependency>
    </dependencies>
</project>
